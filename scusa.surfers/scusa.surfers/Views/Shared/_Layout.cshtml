@section css {
    @Styles.Render(
        "~/Content/RoundHere/map-view.css",
        "~/Content/RoundHere/style.css"

        )

}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - RoundHere</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <link href="~/Content/font-awesome-4.1.0/css/font-awesome.min.css" rel="stylesheet">
    @Styles.Render("~/Content/css", "~/Content/RoundHere/map-view.css", "~/Content/RoundHere/style.css")
    @Scripts.Render("~/bundles/modernizr", "~/Scripts/RoundHere/map-view.js")

</head>
<body>
    <div class="container">
        <!-- Static navbar -->
        <!-- Fixed navbar -->
        <!--<div class="navbar navbar-inverse navbar-fixed-top" role="navigation"> -->
        <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="/Home/"><i class="fa fa-bullseye fa-lg"></i>Round Here</a>
                </div>
                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">

						
                        <li class="dropdown">
                            <a href="@Url.Action("../Create")"><i class="fa fa-plus fa-2x"></i></a>
                        </li>



                    </ul>
					@using (Html.BeginForm("SearchQueryExecution", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
					 {
                    <div id="custom-search-input">
							 <div class="input-group col-md-3">
                                <input type="text" id="searchInput" name="searchinput" class="search-query form-control" placeholder="Search" />
                                <span class="input-group-btn">
                                    <button class="btn btn-danger" type="button">
                                        <span class=" glyphicon glyphicon-search"></span>
                                    </button>
                                </span>
                            </div>
                    </div>
					 }

                    <ul class="nav navbar-nav navbar-right">
                        <li class="dropdown user-dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i>SCUSA USER <b class="caret"></b></a>
                            <ul class="dropdown-menu">
                                <li><a target="_blank" href="http://simkeyur.com/"><i class="fa fa-user"></i>Profile</a></li>
                                <li><a href="#"><i class="fa fa-gear"></i>Settings</a></li>
                                <li class="divider"></li>
                                <li><a href="#"><i class="fa fa-power-off"></i>Log Out</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <!--/.nav-collapse -->
            </div>
        </div>




    </div>


    @RenderBody()
</body>

@Scripts.Render("~/Scripts/jquery-1.9.1.js")
@Scripts.Render("~/Scripts/jquery-ui-1.8.20.js")
@Scripts.Render("~/Scripts/bootstrap.min.js")
@Scripts.Render("~/Scripts/knockout-2.1.0.debug.js")
@Scripts.Render("~/Scripts/knockout.validation.js")
@Scripts.Render("~/Scripts/RoundHere/AddReview.js")
@Scripts.Render("~/Scripts/jquery.maskedinput-1.3.1.min_.js")
	
@RenderSection("scripts", required: false)
@Scripts.Render("https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false&callback=initialize")



</html>
